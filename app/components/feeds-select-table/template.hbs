<div class="list-group">
  {{#if (eq '' selectedFeed)}}
    <button type="button" class="list-group-item active" {{action "feedClicked" null}}>
      All
    </button>
  {{else}}
  <button type="button" class="list-group-item" {{action "feedClicked" null}}>
    All
  </button>
  {{/if}}
  {{#each feeds as |feed|}}
    {{#if (eq feed.onestop_id selectedFeed)}}
    <button type="button" class="list-group-item active" {{action "feedClicked" feed}}>
      {{feed.onestop_id}}
    </button>
    {{else}}
      <button type="button" class="list-group-item" {{action "feedClicked" feed}}>
        {{feed.onestop_id}}
      </button>
    {{/if}}
  {{/each}}
</div>
